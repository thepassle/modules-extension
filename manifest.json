{
  "manifest_version": 3,
  "name": "Module graph",
  "version": "1.0",
  "description": "Module graph inspector",
  "permissions": [
    "debugger",
    "tabs",
    "webRequest",
    "webNavigation",
    "<all_urls>"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js"
  }
}